<?xml version="1.0" encoding="utf-8" ?>

<configuration>

<startup>

<supportedRuntimeversion="v4.0"sku=".NETFramework,Version=v4.7.2" />

</startup>

<system.net>

<connectionManagement>

<add maxconnection = "200" address ="*" />

</connectionManagement>

</system.net>

<system.serviceModel>

<bindings>

<netTcpBinding>

<bindingmaxReceivedMessageSize="80000000"openTimeout="00:10:00"

closeTimeout="00:10:00"

sendTimeout="00:10:00"

receiveTimeout="00:10:00"/>

</netTcpBinding>

</bindings>

<client>

<endpointaddress="net.tcp://localhost:8090/ItalianPizza/Backend"binding="netTcpBinding"contract="Backend.Service.IItalianPizzaService" />

</client>

</system.serviceModel>

</configuration>